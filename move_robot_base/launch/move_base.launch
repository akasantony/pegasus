<launch>

   <master auto="start"/>
   <node pkg="tf" type="static_transform_publisher" name="map_odom_broadcaster"
         args="0 0 0 0 0 0 map odom 100"/>

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find move_robot_base)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find move_robot_base)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find move_robot_base)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find move_robot_base)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find move_robot_base)/config/base_local_planner_params.yaml" command="load" />
 </node>

</launch> 
